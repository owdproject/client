{
  "name": "my-module",
  "version": "3.1.0-alpha.0",
  "keywords": [
    "web",
    "desktop",
    "vue",
    "nuxt",
    "web-desktop",
    "web-os"
  ],
  "homepage": "https://github.com/owdproject/client#readme",
  "bugs": {
    "url": "https://github.com/owdproject/client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owdproject/client.git"
  },
  "license": "GPL-3.0-or-later",
  "author": {
    "name": "dxlliv",
    "url": "https://github.com/dxlliv"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.2",
    "@nuxt/fonts": "^0.11.0",
    "@nuxt/icon": "^1.11.0",
    "@nuxtjs/i18n": "^9.4.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@pinia/nuxt": "^0.10.1",
    "@primeuix/themes": "^1.0.0",
    "@primevue/forms": "^4.3.3",
    "@primevue/nuxt-module": "^4.3.3",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0",
    "@vueuse/nuxt": "^13.0.0",
    "memfs": "^4.17.0",
    "mitt": "^3.0.1",
    "nanoid": "^5.1.5",
    "nuxt": "^3.16.1",
    "pinia": "^3.0.1",
    "sanitize.css": "^13.0.0",
    "sass": "^1.86.0",
    "vue-resizable": "^2.1.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.2",
    "@nuxt/test-utils": "^3.18.0",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "typescript": "~5.8.3",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  },
  "resolutions": {
    "vite": "^6.3.2"
  }
}
