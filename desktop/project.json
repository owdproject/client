{
  "name": "desktop",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm install && pnpm run dev"
        ],
        "cwd": "desktop"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm install && pnpm run generate"
        ],
        "cwd": "desktop"
      }
    }
  }
}
